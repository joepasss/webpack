{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"printWidth": 80,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"jsxBracketSameLine": true,
	"embeddedLanguageFormatting": "auto",
	"jsxSingleQuote": false,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"requirePragma": false,
	"insertPragma": false
}
